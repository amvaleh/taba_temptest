<p id="notice"><%= notice %></p>

<h1>Listing Bluetooth Devices</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Sampling times</th>
      <th>Location</th>
      <th>Version</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bluetooth_devices.each do |bluetooth_device| %>
      <tr>
        <td><%= bluetooth_device.user_id %></td>
        <td><%= bluetooth_device.sampling_times %></td>
        <td><%= bluetooth_device.location %></td>
        <td><%= bluetooth_device.version %></td>
        <td><%= bluetooth_device.status %></td>
        <td><%= link_to 'Show', bluetooth_device %></td>
        <td><%= link_to 'Edit', edit_bluetooth_device_path(bluetooth_device) %></td>
        <td><%= link_to 'Destroy', bluetooth_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bluetooth device', new_bluetooth_device_path %>
