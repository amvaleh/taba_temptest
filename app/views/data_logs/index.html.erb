<p id="notice"><%= notice %></p>

<h1>Listing Data Logs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Plant</th>
      <th>Humidity soil</th>
      <th>Humidity air</th>
      <th>Light</th>
      <th>Temperature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_logs.each do |data_log| %>
      <tr>
        <td><%= data_log.user_id %></td>
        <td><%= data_log.plant_id %></td>
        <td><%= data_log.humidity_soil %></td>
        <td><%= data_log.humidity_air %></td>
        <td><%= data_log.light %></td>
        <td><%= data_log.temperature %></td>
        <td><%= link_to 'Show', data_log %></td>
        <td><%= link_to 'Edit', edit_data_log_path(data_log) %></td>
        <td><%= link_to 'Destroy', data_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data log', new_data_log_path %>
